    <PackageTags>$(PackageTags);build;sdk;msbuild;ci;continous integration</PackageTags>
  </PropertyGroup>
    <ItemGroup>
        <Using Remove="System.Diagnostics.CodeAnalysis.NotNullAttribute" />
