      <ProjectReference Include="..\GlobalAnalyzerConfig\Rocket.Surgery.MSBuild.GlobalAnalyzerConfig.csproj" />
      <ProjectReference Include="..\SourceLink\Rocket.Surgery.MSBuild.SourceLink.csproj" />
    </ItemGroup>
</Project>
